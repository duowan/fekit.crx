{
    "manifest_version": 2,

    "name": "fekit",
    "description": "f2e toolkit",
    "version": "0.0.1",

    "permissions": [
        "tabs",
        "clipboardRead",
        "clipboardWrite"
    ],
    "browser_action": {
        "default_icon": "img/icon32.png",
        "default_popup": "main.html",
        "icons": { 
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png" 
        }
    }
}